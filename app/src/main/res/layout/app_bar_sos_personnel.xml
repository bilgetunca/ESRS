<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SosPersonnelActivity">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/CustomAppBarStyle">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="@color/maintextcolor"
            app:logo="@drawable/ic_launcher"
            app:title="ESRS"
            app:contentInsetStart="72dp"
            app:navigationIcon="@drawable/ic_menu_icon"
            app:popupTheme="@style/CustomPopupStyle"
            app:titleTextColor="@android:color/white"
            app:theme="@style/CustomAppBarStyle"
            app:titleTextAppearance="@style/ToolbarTitleBold" />
    </com.google.android.material.appbar.AppBarLayout>

    <include layout="@layout/content_sos_personnel" />

</androidx.coordinatorlayout.widget.CoordinatorLayout> 